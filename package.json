{
  "name": "gameshare-bot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "node build/esbuild.mjs",
    "start": "node dist/index.js",
    "prepare": "husky install",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "commands:register": "tsx src/discord/registerCommands.ts",
    "invite": "tsx scripts/inviteLink.ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "esbuild": "^0.19.0",
    "husky": "^8.0.0",
    "eslint": "^9.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.19.1",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}
